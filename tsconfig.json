{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "declaration": false,
    "target": "es5",
    "lib": ["es5", "es2015", "es6", "es2016", "es2017.object", "es2018.promise", "dom"],
    "downlevelIteration": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./es5",
    "sourceMap": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "lib/**/*.ts",
    "lib/**/*.js",
    "lib/*.ts"
  ]
}
